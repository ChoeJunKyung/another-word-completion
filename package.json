{
  "name": "another-word-completion",
  "displayName": "Another Word Completion",
  "description": "Let Visual Studio Code provides another word completions.",
  "version": "0.0.1",
  "publisher": "getogrand",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "keybindings": [
      {
        "command": "anotherWordCompletion.cycle",
        "key": "alt+/",
        "when": "editorTextFocus && !editorHasMultiSelection && !editorReadOnly"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "lodash": "^4.17.2"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "lodash": "^4.17.2",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "icon": "images/logo.png"
}
